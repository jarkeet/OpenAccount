<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:height="8dp"
        android:id="@android:id/background"
        android:gravity="center">
        <shape>
            <corners android:radius="@dimen/dp_4"/>
            <solid android:color="@color/cfrecordpad_bg_process_normal" />
            <stroke android:width="@dimen/dp_1" android:color="#FFE5E5E5" />
        </shape>
    </item>

    <item
        android:height="8dp"
        android:id="@android:id/progress"
        android:gravity="center">
        <clip>
            <shape>
                <corners android:radius="@dimen/dp_4"/>
                <solid android:color="#FF0B5DFF" />
            </shape>

        </clip>
    </item>
</layer-list>
